package com.Program.Practice;

public class Constructor {

	int a;
	Constructor()
	{
		System.out.println("executing constructor");
		a=56;
	}
	
}

class Test
{	
	public static void main(String[] args) {
		Constructor c1 = new Constructor();
		System.out.println("a="+c1.a);
		new Constructor();
		
		
	}

}
